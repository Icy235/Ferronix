<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MainMenuButton">
        <item name="android:elevation" >20sp</item>
        <item name="android:fontFamily">@font/unbounded</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_height">74sp</item>
        <item name="android:layout_width">294sp</item>
        <item name="icon">@drawable/ic_list</item>

    </style>


    <style name="HistoryBtn">
        <item name="android:elevation" >20sp</item>
        <item name="android:fontFamily">@font/unbounded</item>
        <item name="android:backgroundTint">@color/primary</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_height">53sp</item>
        <item name="android:layout_width">294sp</item>
        <item name="icon">@drawable/ic_history</item>
    </style>


    <style name="BaseText">
        <item name="android:fontFamily">@font/unbounded</item>
        <item name="android:backgroundTint">@android:color/transparent</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>

    </style>


</resources>